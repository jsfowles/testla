{
  "presets": ["@babel/preset-env", "@babel/preset-react", "next/babel"],
  "plugins": [
    [
      "emotion",
      {
        "inline": true,
        "hoist": false,
        "sourceMap": true,
        "autoLabel": true,
        "labelFormat": "[local]",
        "extractStatic": false,
        "outputDir": "",
        "importedNames": {
          "styled": "styled",
          "css": "css",
          "keyframes": "keyframes",
          "injectGlobal": "injectGlobal",
          "merge": "merge"
        }
      }
    ]
  ]
}
